{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "lib": [
            "ES2020"
        ],
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "bundler"
    },
    "include": [
        "src/**/*",
        ".prisma/client/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}